@model IEnumerable<Bd2Domain.Model.Author>

@{
    ViewData["Title"] = "Index";
}

<h1>Результати:</h1>


<form method="post">
<table class="table">
    <thead>
        <tr>
     
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
         
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
         
           
        </tr>
}
    </tbody>
</table>
</form>
